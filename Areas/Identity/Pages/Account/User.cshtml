@page
@model WebApp1.Areas.Identity.Pages.Account.CreateModel
@{
    ViewData["Title"] = "Create";
}

<h1>User</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Computation.ComputationName" class="control-label"></label>
                <input asp-for="Computation.ComputationName" class="form-control" />
                <span asp-validation-for="Computation.ComputationName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Computation.Revenues[0].RevenueName" class="control-label"></label>
                <input asp-for="Computation.Revenues[0].RevenueName" class="form-control" />
                <span asp-validation-for="Computation.Revenues[0].RevenueName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}