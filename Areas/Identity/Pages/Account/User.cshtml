@page
@model WebApp1.Areas.Identity.Pages.Account.CreateModel
@{
    ViewData["Title"] = "Create";
}

<h1>User</h1>
<div class="row">
    <div class="col-md-11">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Computation.ComputationName" class="control-label"></label>
                <input asp-for="Computation.ComputationName" class="form-control" />
                <span asp-validation-for="Computation.ComputationName" class="text-danger"></span>
            </div>
            <div class="container">
                <div class="row">
                    <div class="form-group col-sm">
                        <label asp-for="Computation.Revenues[0].RevenueName" class="control-label"></label>
                        <input asp-for="Computation.Revenues[0].RevenueName" class="form-control" />
                        <span asp-validation-for="Computation.Revenues[0].RevenueName" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm">
                        <label asp-for="Computation.Revenues[0].HourPrice" class="control-label"></label>
                        <input asp-for="Computation.Revenues[0].HourPrice" class="form-control" name="num1" />
                        <span asp-validation-for="Computation.Revenues[0].HourPrice" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm">
                        <label asp-for="Computation.Revenues[0].Hours" class="control-label "></label>
                        <input asp-for="Computation.Revenues[0].Hours" class="form-control" name="num2"/>
                        <span asp-validation-for="Computation.Revenues[0].Hours" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm">
                        <div class="printchatbox" id="theProduct"></div>
                    </div>
                    

                    <div class="form-group col-sm">
                        <label asp-for="Computation.Revenues[1].RevenueName" class="control-label"></label>
                        <input asp-for="Computation.Revenues[1].RevenueName" class="form-control" />
                        <span asp-validation-for="Computation.Revenues[1].RevenueName" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm">
                        <label asp-for="Computation.Revenues[1].HourPrice" class="control-label"></label>
                        <input asp-for="Computation.Revenues[1].HourPrice" class="form-control" />
                        <span asp-validation-for="Computation.Revenues[1].HourPrice" class="text-danger"></span>
                    </div>
                    <div class="form-group col-sm">
                        <label asp-for="Computation.Revenues[1].Hours" class="control-label "></label>
                        <input asp-for="Computation.Revenues[1].Hours" class="form-control" />
                        <span asp-validation-for="Computation.Revenues[1].Hours" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    
}
<script>
    var inputbox1 = document.getElementById('num1');
    var inputbox2 = document.getElementById('num2');
    inputBox1.onkeyup || inputbox2.onkeyup = function{
        var numOne = document.getElementById('num1').value;
        var numTwo = document.getElementById('num2').value;
        var theProduct = parseInt(numOne) * parseInt(numTwo);
        var p = document.getElementById('theProduct');
        p.innerHTML += theProduct;
    }
</script>